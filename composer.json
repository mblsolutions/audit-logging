{
    "name": "mblsolutions/audit-logging",
    "description": "Audit Logging package for Laravel applications",
    "type": "package",
    "license": "proprietary",
    "authors": [
        {
            "name": "MBL Solutions Ltd",
            "email": "tech@mblsolutions.co.uk"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "ext-json": "*",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0|^10|^11",
        "illuminate/events": "^6.0|^7.0|^8.0|^9.0|^10|^11",
        "illuminate/database": "^6.0|^7.0|^8.0|^9.0|^10|^11",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10|^11"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "orchestra/testbench": "^4.0||^5.0||^6.0||^7.0||^8.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "^7.3||^8.2||^9.3||^10.0"
    },
    "autoload": {
        "psr-4": {
            "MBLSolutions\\AuditLogging\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MBLSolutions\\AuditLogging\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "MBLSolutions\\AuditLogging\\AuditLoggingServiceProvider"
            ]
        }
    }
}
