{
    "name": "mblsolutions/audit-logging",
    "description": "Audit Logging package for Laravel applications",
    "type": "package",
    "license": "proprietary",
    "authors": [
        {
            "name": "MBL Solutions Ltd",
            "email": "tech@mblsolutions.co.uk"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "illuminate/contracts": "^10.0|^11.0",
        "illuminate/events": "^10.0|^11.0",
        "illuminate/database": "^10|^11.0",
        "illuminate/support": "^10|^11.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "orchestra/testbench": "^6.0||^7.0||^8.0",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.0||^11.0"
    },
    "autoload": {
        "psr-4": {
            "MBLSolutions\\AuditLogging\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MBLSolutions\\AuditLogging\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "MBLSolutions\\AuditLogging\\AuditLoggingServiceProvider"
            ]
        }
    }
}
